{
    "name": "modix/php-cs-rulesets",
    "description": "Modix rulesets for PHP-CS-Fixer",
    "type": "library",
    "authors": [
        {
            "name": "Andreas Linden",
            "email": "andreas.linden@coxautoinc.com"
        }
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^2.19",
        "composer/composer": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Modix\\PhpCsRulesets\\": "src/"
        }
    },
    "scripts": {
        "modix-cs-configs": [
            "Modix\\PhpCsRulesets\\Composer\\GrumpConfig::create",
            "Modix\\PhpCsRulesets\\Composer\\PhpCsConfig::create"
        ]
    }
}
